// Online Java Compiler
// Use this editor to write, compile and run your Java code online

class HelloWorld {
    
    public static int fun(int n){
        if(n<0) return -1;
        if(n==0) return 1;
        return n*fun(n-1);
    }
    public static void main(String[] args) {
        
        System.out.println(fun(5));
    }
}
