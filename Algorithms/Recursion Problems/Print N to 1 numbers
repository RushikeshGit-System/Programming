// Online Java Compiler
// Use this editor to write, compile and run your Java code online

class HelloWorld {
    
    public static void fun(int n){
        if(n==0) return;
        System.out.println(n);
        fun(n-1);
    }
    public static void main(String[] args) {
        System.out.println("Hello, World!");
        fun(5);
    }
}
